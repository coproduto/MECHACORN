            <string>///Shoot missile
if (missiles &gt; 0)
{
    missiles -= 1;
    
    if (Movable_Side == 180)
    {
        hand_pos = -Movable_Width/2;
    }
    else
    {
        hand_pos = Movable_Width/2;
    }
    var tiro = instance_create(x+hand_pos, y-Movable_Height/2, Obj_Missile);
    if (Movable_Side == 180)
    {
        tiro.image_xscale = -1;
    }
    tiro.direction = Movable_Side;
}
</string>
