if (Damage_Delay &gt; 0)
{
    Damage_Delay -= 1;
    
    //Change sprite to blink
    if (Damage_Delay mod 8 == 7)
    {
        visible = false;
    } 
    else if (Damage_Delay mod 8 == 3)
    {
        visible = true;
    }
}

//Will take damage
if (Control_Damage == 1)
{
    lives -= Damage_Qt;
    Damage_Qt = 0;
    
    //Little jump
    move_contact_solid(90, 1);
    Movable_V_Speed = -Movable_Jump_Speed/2;
    
    Damage_Delay = Player_Damage_Delay;
    Control_Damage = 0;
}
