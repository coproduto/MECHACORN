        <arguments>
          <argument>
            <kind>1</kind>
            <string>///declaration of player's variables

//default Movable declaration
event_inherited();

//Stop the animation
animationDelay = 0;
isAnimationHigh = 0;
image_speed = 0;

//player info
lives = 4;
missiles = 3;

//damage
Damage_Delay = Player_Damage_Delay;
Damage_Qt = 0;
Control_Damage = 0;

//directions
Movable_Facing = 0; //the head is looking at that direction (Up/Down/Left/Right)
Movable_Side = 0; //the body is facing that direction (Left/Right)

//gun position
Movable_Gun_X[0] = 40;
Movable_Gun_Y[0] = -118;
Movable_Gun_X[1] = -10;
Movable_Gun_Y[1] = -138;
Movable_Gun_X[2] = -40;
Movable_Gun_Y[2] = -118;
Movable_Gun_X[3] = 0;
Movable_Gun_Y[3] = 0;


//constants
Movable_Width = Player_Width;
Movable_Jump_Speed = Player_Jump_Speed;
Movable_Bullet_Delay = Bullet_Delay;
Movable_Bullet_Speed = Bullet_Speed;
Movable_Bullet_Deviation = Bullet_Deviation;
Movable_Obj_Tiro = Obj_Player_Bullet1;
Movable_H_Speed = Player_H_Speed; //Player Horizontal Speed
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Moviment and gun

var needToReflect = Movable_Side;
var indexModule = (image_index % 4);
var old_x = x;
if (needToReflect == 0) //reflect gun position
{
    Movable_Gun_X[1] = -Movable_Gun_X[1];
    Movable_Gun_X[3] = -Movable_Gun_X[3];
}

if (x &lt; Movable_Width/2 &amp;&amp; Control_Horizontal == -1)
{   //left wall
    Control_Horizontal = 0;
}
else if (x &gt; room_width - Movable_Width/2 &amp;&amp; Control_Horizontal == 1)
{   //right wall
    Control_Horizontal = 0;
}

//default Movable End Step
event_inherited();

if (old_x == x) //player don't moved horizontaly
{
    Control_Horizontal = 0;
}

if (needToReflect == 0) //undo gun reflection
{
    Movable_Gun_X[1] = -Movable_Gun_X[1];
    Movable_Gun_X[3] = -Movable_Gun_X[3];
}

//Jig
if (Control_Vertical == 1) //Player Squating
{
    sprite_index = Spr_Player_Squating;
    image_index = 0;
     
     if (isAnimationHigh == 1)
     {
        isAnimationHigh = 0;
        Movable_Gun_Y[0] += 14;
        Movable_Gun_Y[1] += 14;
        Movable_Gun_Y[2] += 14;   
     }
}
else
{
    sprite_index = Spr_Player;
    image_index = indexModule;
    if (Control_Vertical == -1) //looking up
    {
        image_index += 8;
    }
    else if (Control_Shoot == 1) //shooting gun
    {
        image_index += 4;
    }

    
    if (Control_Horizontal == 0)
    {
        image_index -= indexModule;
        animationDelay = 0;
    }
    else
    {
        if (animationDelay == 0)
        {
            animationDelay = 3;
            Scr_Jig();
        }
        else
        {
            animationDelay -= 1;
        }
    }
    
    indexModule = image_index % 4;
    if (indexModule == 0 || indexModule == 3)
    {
        if (isAnimationHigh == 1)
        {
            isAnimationHigh = 0;
            Movable_Gun_Y[0] += 14;
            Movable_Gun_Y[1] += 14;
            Movable_Gun_Y[2] += 14;
        }
    }
    else if (isAnimationHigh == 0)
    {
        isAnimationHigh = 1;
        Movable_Gun_Y[0] -= 14;
        Movable_Gun_Y[1] -= 14;
        Movable_Gun_Y[2] -= 14;   
    }
}
</string>
          </argument>
        </arguments>
      </action>
